{
  "name": "@banez/vue-array-store",
  "version": "1.2.0",
  "description": "Simple array store for Vue 3.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "vite",
    "build:ts": "tsc -p tsconfig.bundle.json",
    "bundle": "npm run lint && npm-tool --bundle",
    "publish": "npm-tool --publish",
    "link": "npm-tool --link",
    "link:sudo": "npm-tool --link --sudo",
    "unlink": "npm-tool --unlink",
    "unlink:sudo": "npm-tool --unlink --sudo",
    "pack": "npm-tool --pack",
    "test": "mocha -r ts-node/register test/integration/*.test.ts --recursive --exit",
    "test:active": "mocha -r ts-node/register test/integration/*.active.test.ts --recursive --exit --bail",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts --recursive --exit",
    "test:unit:active": "mocha -r ts-node/register test/unit/*.active.test.ts --recursive --exit",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bbanez/vue-array-store.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "vuex",
    "store"
  ],
  "author": "Branislav VajagiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bbanez/vue-array-store/issues"
  },
  "homepage": "https://github.com/Bbanez/vue-array-store",
  "peerDependencies": {
    "vue": ">=3.2.25"
  },
  "devDependencies": {
    "@banez/npm-tool": "1.1.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^15.6.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "chai": "^4.3.4",
    "eslint": "^7.9.0",
    "mocha": "^9.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.54.9",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "vite": "^2.7.13",
    "vite-plugin-vue-router": "^1.0.18",
    "vite-tsconfig-paths": "^3.5.0",
    "vue": "^3.2.25",
    "vue-tsc": "^0.31.1"
  }
}
